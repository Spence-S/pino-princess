{
  "name": "pino-princess",
  "version": "0.4.0",
  "description": "Prettiest Pino Prettifier in all the land",
  "keywords": [
    "pino",
    "pino-pretty",
    "ndjson",
    "prettifier",
    "pino-colada",
    "pretty",
    "prettify"
  ],
  "homepage": "https://github.com/spence-s/pino-princess",
  "bugs": {
    "url": "https://github.com/spence-s/pino-pretty/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/spence-s/pino-princess.git"
  },
  "license": "MIT",
  "author": "Spencer Snyder <sasnyde2@gmail.com>",
  "type": "commonjs",
  "main": "dist/index.js",
  "bin": {
    "pino-princess": "./dist/cli.js"
  },
  "files": [
    "dist",
    "!dist/test"
  ],
  "scripts": {
    "build": "npm run clean && tsc --project tsconfig.json",
    "clean": "rm -rf dist",
    "lint": "xo",
    "test": "ava",
    "watch": "npm run clean && tsc --project tsconfig.json --watch"
  },
  "xo": {
    "parserOptions": {
      "project": "./tsconfig.json"
    },
    "prettier": true,
    "rules": {
      "unicorn/prefer-module": "off",
      "import/extensions": "off",
      "n/file-extension-in-import": "off",
      "unicorn/no-array-reduce": "off",
      "unicorn/prefer-object-from-entries": "off",
      "no-negated-condition": "off",
      "unicorn/prevent-abbreviations": "off",
      "capitalized-comments": "off",
      "ava/use-test": "off"
    },
    "space": true
  },
  "ava": {
    "environmentVariables": {
      "NODE_OPTIONS": "-r ts-node/register/transpile-only"
    },
    "extensions": [
      "ts"
    ],
    "files": [
      "test/**/*"
    ],
    "verbose": true
  },
  "dependencies": {
    "chalk": "^4",
    "cli-highlight": "^2.1.11",
    "cosmiconfig": "^8.3.6",
    "dayjs": "^1.11.9",
    "fast-json-parse": "^1.0.3",
    "get-value": "^3.0.1",
    "json-stringify-pretty-compact": "^3.0.0",
    "meow": "^12.1.1",
    "on-exit-leak-free": "^2.1.0",
    "pino-abstract-transport": "^1.1.0",
    "pretty-ms": "^7",
    "pump": "^3.0.0",
    "readable-stream": "^4.4.2",
    "set-value": "^4.1.0",
    "sonic-boom": "^3.3.0",
    "unset-value": "^2.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@types/events": "^3.0.0",
    "@types/get-value": "^3.0.3",
    "@types/node": "^20.6.0",
    "@types/pump": "^1.1.1",
    "@types/set-value": "^4.0.1",
    "ava": "^5.3.1",
    "fixpack": "^4.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "pino": "^8.15.1",
    "prettier-plugin-packagejson": "^2.4.5",
    "strip-ansi": "^7.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "xo": "^0.56.0"
  },
  "engines": {
    "node": ">=16"
  }
}
